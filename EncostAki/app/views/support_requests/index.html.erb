<p id="notice"><%= notice %></p>

<h1>Support Requests</h1>

<table>
  <thead>
    <tr>
      <th>Ano</th>
      <th>Mes</th>
      <th>Processo numero</th>
      <th>Solicitacao data</th>
      <th>Solicitacao hora</th>
      <th>Solicitacao descricao</th>
      <th>Solicitacao regional</th>
      <th>Solicitacao bairro</th>
      <th>Solicitacao localidade</th>
      <th>Solicitacao endereco</th>
      <th>Solicitacao roteiro</th>
      <th>Rpa codigo</th>
      <th>Rpa nome</th>
      <th>Solicitacao microrregiao</th>
      <th>Solicitacao plantao</th>
      <th>Solicitacao origem chamado</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Solicitacao vitimas</th>
      <th>Solicitacao vitimas fatais</th>
      <th>Processo tipo</th>
      <th>Processo origem</th>
      <th>Processo localizacao</th>
      <th>Processo status</th>
      <th>Processo data conclusao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @support_requests.each do |support_request| %>
      <tr>
        <td><%= support_request.ano %></td>
        <td><%= support_request.mes %></td>
        <td><%= support_request.processo_numero %></td>
        <td><%= support_request.solicitacao_data %></td>
        <td><%= support_request.solicitacao_hora %></td>
        <td><%= support_request.solicitacao_descricao %></td>
        <td><%= support_request.solicitacao_regional %></td>
        <td><%= support_request.solicitacao_bairro %></td>
        <td><%= support_request.solicitacao_localidade %></td>
        <td><%= support_request.solicitacao_endereco %></td>
        <td><%= support_request.solicitacao_roteiro %></td>
        <td><%= support_request.rpa_codigo %></td>
        <td><%= support_request.rpa_nome %></td>
        <td><%= support_request.solicitacao_microrregiao %></td>
        <td><%= support_request.solicitacao_plantao %></td>
        <td><%= support_request.solicitacao_origem_chamado %></td>
        <td><%= support_request.latitude %></td>
        <td><%= support_request.longitude %></td>
        <td><%= support_request.solicitacao_vitimas %></td>
        <td><%= support_request.solicitacao_vitimas_fatais %></td>
        <td><%= support_request.processo_tipo %></td>
        <td><%= support_request.processo_origem %></td>
        <td><%= support_request.processo_localizacao %></td>
        <td><%= support_request.processo_status %></td>
        <td><%= support_request.processo_data_conclusao %></td>
        <td><%= link_to 'Show', support_request %></td>
        <td><%= link_to 'Edit', edit_support_request_path(support_request) %></td>
        <td><%= link_to 'Destroy', support_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2> Import </h2>
<%= form_for :dump, :url => import_request_path, :html => { :multipart => true} do |f| -%>
  <table>
    <tr>
      <td>
        <label for = "dump_file">
          Selecione um arquivo CSV:
        </label>
      </td>
      <td>
        <%= f.file_field :file -%>
      </td>
    </tr>
    <tr>
      <td colspan='2'>
        <%= submit_tag 'Import' -%>
      </td>
    </tr>
  </table>
<% end -%>

<br>

<%= link_to 'New Support Request', new_support_request_path %>
<p>Voltar ao <%= link_to "Menu Principal", root_path %></p>
