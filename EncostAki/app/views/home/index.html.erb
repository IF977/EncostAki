        <div id="esquerda">
        <div id="map"></div>
        <script>
            function initMap() {
                var cin = {lat: -8.055590, lng: -34.951344};
                var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 10,
                center: cin
            });
            var marker = new google.maps.Marker({
                position: cin,
                map: map
            });

            var contentString = '<h1>Teste</h1>';
            var infowindow = new google.maps.InfoWindow({
                content: contentString
            });
                marker.addListener('click', function() {
                     infowindow.open(map, marker);
             });
            
        }
        </script>
        </div>
        <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAVW_TaNxAKo2ATMVcdf2Q1YQlFe_H7-zY&callback=initMap">
